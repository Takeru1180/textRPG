def training():
   global training_cost
   hero["所持金"] = int(hero["所持金"]) - training_cost
   training_rundom = random.randint(0,100)
   if  0<= training_rundom <= 5:
     training_1 = 3
     training_2 = 5
     print("超大成功！！！")
   if  6 <= training_rundom <= 15: 
     training_1 = 1.5
     training_2 = 3
     print("超成功！！")
   if  16 <= training_rundom <= 30: 
     training_1 = 3
     training_2 = 2
     print("大成功！")
   if  31<= training_rundom <= 100:
     training_1 = 2
     training_2 = 1
     print("成功！")
   HP_UP  = int(int(hero_kiso["HP"]) * (100 + int(hero["レベル"]))//100 * training_2)
   MP_UP  = int(int(hero_kiso["MP"]) * (100 + int(hero["レベル"]))//100 * training_2)
   ATK_UP = int(int(hero_kiso["攻撃力"]) * (100 + int(hero["レベル"]))//100 * training_2)
   DEF_UP = int(int(hero_kiso["防御力"]) * (100 + int(hero["レベル"]))//100 * training_2)
   SPD_UP = int(int(hero_kiso["防御力"]) * (100 + int(hero["レベル"]))//100 * training_2)
   hero["最大HP"] = int(hero["最大HP"]) + int(HP_UP)
   hero["最大MP"] = int(hero["最大MP"]) + int(MP_UP)
   hero["攻撃力"] = int(hero["攻撃力"]) + int(ATK_UP)
   hero["防御力"] = int(hero["防御力"]) + int(DEF_UP)
   hero["素早さ"] = int(hero["素早さ"]) + int(SPD_UP)   
   print("HPが"+str(HP_UP)+"上がった！")
   print("MPが"+str(MP_UP)+"上がった！")
   print("攻撃力が"+str(ATK_UP)+"上がった！")
   print("防御力が"+str(DEF_UP)+"上がった！")
   print("素早さが"+str(SPD_UP)+"上がった！")
   print("")
   training_cost = training_cost + 20 * int(hero["レベル"])

