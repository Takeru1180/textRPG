def search():
  global eria_choice
  global enemy
  while True:
   print("現在の進行度は、"+str(sinkoudo[eria_choice])+"%です")
   if int(sinkoudo[eria_choice]) < 100:
     print("1:先に進む(進行度＋20～50%)")
   if int(sinkoudo[eria_choice]) >= 100:
     print("1:魔族と戦う")
   print("2:町に戻る")
   if int(sinkoudo[eria_choice]) >=100 and int(boss_kill[eria_choice]) == 0 or int(sinkoudo[eria_choice]) >=100 and int(boss_win[eria_choice]):
     print("3:エリアボスの元へ向かう")
   search_comand = input()
   if int(search_comand) == 1:
     random_search = random.randint(1,100)
     if 1 <= random_search <= 10 or int(sinkoudo[eria_choice]) >= 100:
       encount()
       return
     elif 11<= random_search <= 90:
       random_sinkoudo = random.randint(20,50)
       sinkoudo[eria_choice] = int(sinkoudo[eria_choice])+int(random_sinkoudo)
       if int(sinkoudo[eria_choice]) >=100:
         sinkoudo[eria_choice] =100
       print("進行度が"+str(random_sinkoudo)+"%上がった！")
       print("")
     elif 91<= random_search <= 100:
        random_gold = int(hero["レベル"]) * random.randint(1000,10000)
        hero["所持金"] = int(hero["所持金"]) + int(random_gold)
        print(str(hero["名前"])+"は宝箱を見つけた！")
        print(str(hero["名前"])+"は"+str(random_gold)+"Gを手に入れた！")
        print("")
   elif int(search_comand) == 2:
     return
   elif int(search_comand) == 3 and int(sinkoudo[eria_choice]) >=100:
     story()
     return

