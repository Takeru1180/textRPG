def skill():
     global tujyou_atk
     global hero_use_skill
     HERO_ATK = (int(hero["攻撃力"])+int(hero["武器攻撃力"])) * (100+int(hero["攻撃力倍率"]))/100 * (100 + int(hero_buff["攻撃力"]))/100 * (100 - int(hero_debuff["攻撃力"]))/100 
     meityu_random_hero    = random.randint(1,100)
     kaihi_random_enemy  = random.randint(1,100)
     kaisin_random_hero    = random.randint(1,100)
     hero_goukei_damege    = 0
     if tujyou_atk == 0:
       print("何を使用しますか？")
       if skill_flag["1"] == "0":
         print("1:"+str(skill_006))
       if skill_flag["2"] == "0":
         print("2:"+str(skill_012))
       skill_choice = input()
       if int(skill_choice) == 1:
          hero_use_skill = skill_006
       if int(skill_choice) == 2:
          hero_use_skill = skill_012
     if hero["現在MP"] >= int(hero_use_skill["消費MP"]):
       if hero_use_skill["分類"] == "攻撃":
        hero["現在MP"] = int(hero["現在MP"]) - int(hero_use_skill["消費MP"])
        print(str(hero["名前"])+"の"+str(hero_use_skill["名前"])+"!")
        print("")
        hero_damege = int(((int(HERO_ATK)- int(enemy_1["防御力"])) * int(hero_use_skill["威力"])/100) * (100-int(enemy_1["ダメージカット率"]))/100)
        hero_kaisin_damege = int(int(HERO_ATK) * int(hero_use_skill["威力"])/100 * int(hero["会心倍率"]))
        for h in range(int(hero_use_skill["攻撃回数"])):
         meityu_random_hero    = random.randint(1,100)
         kaihi_random_enemy  = random.randint(1,100)
         kaisin_random_hero    = random.randint(1,100)
         if int(hero_damege) <= 0:
           hero_damege = int(hero["レベル"])
         if meityu_random_hero <= int(hero["命中率"]) and kaihi_random_enemy > int(hero["回避率"]):
          if kaisin_random_hero <= int(hero["会心率"]):
           enemy_1["現在HP"] = int(enemy_1["現在HP"]) - int(hero_kaisin_damege) 
           print("クリティカル！")
           print(str(hero_kaisin_damege)+"ダメージ！")
           hero_goukei_damege = int(hero_goukei_damege) + int(hero_kaisin_damege) 
          else:
           enemy_1["現在HP"] = int(enemy_1["現在HP"]) - int(hero_damege)
           print(str(hero_damege)+"ダメージ！")
           hero_goukei_damege = int(hero_goukei_damege) + int(hero_damege) 
         else:
          print(str(enemy_1["名前"])+"は攻撃を回避した！") 
        print(str(hero["名前"])+"は合計"+str(hero_goukei_damege)+"のダメージを与えた！")
        if int(enemy_1["現在HP"]) < 0:
            enemy_1["現在HP"] = 0
        print(str(enemy_1["名前"])+"の現在HPは"+str(enemy_1["現在HP"])+"です！")
        print("")
       elif hero_use_skill["分類"] == "支援":
        hero["現在MP"] = int(hero["現在MP"]) - int(hero_use_skill["消費MP"])
        taisyou = hero_use_skill["対象"]
        hero_buff[taisyou] = int(hero_buff[taisyou]) + int(hero_use_skill["威力"])
        print(str(hero["名前"])+"の"+str(hero_use_skill["名前"])+"!")
        print(str(hero["名前"])+"の"+str(taisyou)+"が"+str(hero_use_skill["威力"])+"%上がった！")
       elif hero_use_skill["分類"] == "回復":
        hero["現在MP"] = int(hero["現在MP"]) - int(hero_use_skill["消費MP"])
        heel_HP = (int(hero["最大HP"]) //100) * int(hero_use_skill["威力"])
        hero["現在HP"] = hero["現在HP"] + heel_HP
        print(str(hero["名前"])+"の"+str(hero_use_skill["名前"])+"!")
        print(str(hero["名前"])+"のHPが"+str(heel_HP)+"回復した！")
     else:
       print("MPが足りない！")

def enemy_turn():
    global boss_syokai
    global tousou
    global enemy
    enemy_AI = ai_matome[int(enemy_1["AI"])]
    HERO_DEF = (int(hero["防御力"])+ int(hero["防具防御力"])) * (100 + int(hero_buff["防御力"]))/100 * (100 - int(hero_debuff["防御力"]))/100
    kaihi_random_hero     = random.randint(1,100)
    meityu_random_enemy   = random.randint(1,100)
    kaisin_random_enemy   = random.randint(1,100)
    enemy_goukei_damege   = 0
    ENEMY_ATK     = int(enemy_1["攻撃力"])
    enemy_use_skill = enemy_AI[2]
    enemy_damege = (int(ENEMY_ATK) - int(HERO_DEF)) * int(enemy_use_skill["威力"])/100 * (100-int(hero["ダメージカット率"]))/100
    if int(boss_syokai) == 0 and int(enemy_1["ボス"]) == 1:
      enemy_use_skill = skill_016
      boss_syokai = 1
    elif int(enemy_1["現在HP"]) <= int(enemy_1["最大HP"]) * 0.5:
      enemy_use_skill = enemy_AI[4]
    elif int(enemy_damege) <= 0:
      enemy_use_skill = enemy_AI[3]
      if int(enemy_1["現在MP"]) <= int(enemy_use_skill["消費MP"]):
        enemy_use_skill = enemy_AI[2]
        if int(enemy_1["現在MP"]) <= int(enemy_use_skill["消費MP"]):
          enemy_use_skill = enemy_AI[1]
          if int(enemy_1["現在MP"]) <= int(enemy_use_skill["消費MP"]):
            enemy_use_skill = enemy_AI[0]
    elif int(enemy_1["現在MP"]) <= int(enemy_use_skill["消費MP"]):
      enemy_use_skill = enemy_AI[1]
      if int(enemy_1["現在MP"]) <= int(enemy_use_skill["消費MP"]):
