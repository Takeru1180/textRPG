def battle():
   global enemy
   global random_force
   global boss_syokai
   global eria_choice
   global sinkoudo_1
   global tousou
   global tujyou_atk
   global hero_use_skill
   if enemy["ボス"] == "0":
     enemy_1["名前"]   = enemy["名前"]
     enemy_1["レベル"] = (int(hero["レベル"]) + int(enemy["レベル"]))//2
     enemy_1["最大HP"] = int(enemy["基礎HP"]) * int(enemy_1["レベル"]) * (int(random_force)/100)
     enemy_1["最大MP"] = int(enemy["基礎MP"]) * int(enemy_1["レベル"]) * (int(random_force)/100)
     enemy_1["攻撃力"] = int(enemy["基礎攻撃力"]) * int(enemy_1["レベル"]) * (int(random_force)/100)
     enemy_1["防御力"] = int(enemy["基礎防御力"]) * int(enemy_1["レベル"]) * (int(random_force)/100)
     enemy_1["素早さ"] = int(enemy["基礎素早さ"]) * int(enemy_1["レベル"]) * (int(random_force)/100)
     enemy_1["討伐報酬"] = int(int(enemy["基礎討伐報酬"]) * int(enemy_1["レベル"]) * (int(random_force)/100))
     enemy_1["経験値"] = int(int(enemy["基礎経験値"]) * int(enemy_1["レベル"]) * (int(random_force)/100))
     enemy_1["現在HP"] = int(enemy_1["最大HP"])
     enemy_1["現在MP"] = int(enemy_1["最大MP"])
     enemy_1["会心率"] = int(enemy["会心率"]) * (int(random_force)/100)
     enemy_1["会心倍率"] = int(enemy["会心倍率"]) * (int(random_force)/100)
     enemy_1["回避率"] = int(enemy["回避率"]) * (int(random_force)/100)
     enemy_1["命中率"] = int(enemy["命中率"]) * (int(random_force)/100)
     enemy_1["ダメージカット率"] = int(enemy["ダメージカット率"])
     enemy_1["AI"]     = enemy["AI"]
     enemy_1["ボス"]   = enemy["ボス"]
   elif enemy["ボス"] == "1":
     boss_buff = int(enemy_kill[eria_choice])
     enemy_1["名前"]   = enemy["名前"]
     enemy_1["レベル"] = int(enemy["レベル"])
     enemy_1["最大HP"] = int(enemy["基礎HP"]) + int(enemy["補正値1"]) * int(boss_buff)
     enemy_1["最大MP"] = int(enemy["基礎MP"]) + int(enemy["補正値1"]) * int(boss_buff)
     enemy_1["攻撃力"] = int(enemy["基礎攻撃力"]) + int(enemy["補正値2"]) * int(boss_buff)
     enemy_1["防御力"] = int(enemy["基礎防御力"]) + int(enemy["補正値2"]) * int(boss_buff)
     enemy_1["素早さ"] = int(enemy["基礎素早さ"]) + int(enemy["補正値2"]) * int(boss_buff)
     enemy_1["討伐報酬"] = int(enemy["基礎討伐報酬"])  + int(enemy["基礎討伐報酬"]) * int(boss_buff)
